    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.149",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-draft-wysiwyg": "^1.13.0",
    "markdown-toc": "^1.2.0",
